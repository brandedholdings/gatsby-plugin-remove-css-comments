{
  "github": {
    "release": true
  },
  "increment": "conventional:angular",
  "scripts": {
    "beforeStage": "npm run build",
    "afterRelease": "echo Successfully released ${name} v${version} to ${repo.repository}."
  },
  "git": {
    "tagName": "v${version}",
    "requireCleaningWorkingDir": true
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "npm": {
    "publish": true
  }
}
